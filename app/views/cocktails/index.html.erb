<div class="container">
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <h1>My Cocktails list</h1>

      <ul>
        <% @cocktails.each do |cocktail| %>
          <li><%= link_to cocktail.name, cocktail_path(cocktail) %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>


